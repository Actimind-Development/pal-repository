{
    "languages": {
        "java8": {
            "path": {
                "prefix": "pal/"
            },
            "code": {
                "extension": "java",
                "methodNaming": "camel",
                "classNaming": "Camel",
                "fileNaming": "Camel",
                "comment": "//",
                "indent": 4
            },
            "templates": {
                "class": "
                    |package ${java.package};
                    |//::imports
                    |
                    |//::module ${tag}
                    |public class ${class} {
                    |    //::addbefore
                    |}
                ",
                "import": "import static ${java.package}.${class}.*;",
                "call": "${class}.${method}(_)"
            }
        },
        "javascript5": {
            "code": {
                "extension": "js",
                "methodNaming": "camel",
                "classNaming": "Camel",
                "fileNaming": "camel",
                "comment": "//",
                "indent": 2
            },
            "templates": {
                "class": "
                    |(function(exports, imports) {
                    |    //::imports
                    |    //::module ${tag}
                    |    exports.${class} = {
                    |    //::addbefore
                    |        stub: null
                    |    }
                    |})(window, window);
                    ",
                "import": "var ${tag} = imports.${class};",
                "call": "window.${class}.${method}(_)"
            }
        }
    }
}