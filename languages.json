{
    "languages": {
        "java8": {
            "path": {
                "prefix": "pal/"
            },
            "code": {
                "extension": "java",
                "methodNaming": "camel",
                "classNaming": "Camel",
                "fileNaming": "Camel",
                "comment": "//",
                "indent": 4
            },
            "templates": {
                "class": "
                    | package ${java.package};
                    | //::imports
                    |
                    | //::module ${tag}
                    | public class ${class} {
                    |  //::addbefore
                    | }
                ",
                "import": "import static ${java.package}.${class}.*;"
            }
        },
        "javascript5": {
            "code": {
                "extension": "js",
                "methodNaming": "camel",
                "classNaming": "Camel",
                "fileNaming": "camel",
                "comment": "//",
                "indent": 2
            },
            "templates": {
                "class": "
                    | (function(window) {
                    |  //::imports
                    |  //::module ${tag}
                    |  window.${class} = {
                    |   //::addbefore
                    |   stub: null
                    |  }
                    | })(window);
                    ",
                "import": "var ${tag} = window.${class};"
            }
        }
    }
}